  "scripts": {
    "compile": "hardhat compile && forge build",
    "test:hardhat": "yarn compile && hardhat test",
    "test:gas": "UPDATE_SNAPSHOT=1 yarn test --grep gas",
    "test:all": "UPDATE_SNAPSHOT=1 yarn test:hardhat && forge test --isolate",
