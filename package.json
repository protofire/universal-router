    "@uniswap/v4-periphery": "^1.0.3",
    "solmate": "^6.8.0"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "1.0.4",
    "@nomicfoundation/hardhat-foundry": "1.1.2",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@typechain/ethers-v5": "^4.0.0",
    "@types/chai": "^4.2.6",
    "@types/mocha": "^5.2.7",
