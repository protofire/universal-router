  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "version": "2.0.0",
  "keywords": [
    "uniswap",
    "router",
    "universal router",
    "swap router"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Uniswap/universal-router"
  },
  "files": [
    "contracts/base",
    "contracts/interfaces",
    "contracts/libraries",
    "contracts",
